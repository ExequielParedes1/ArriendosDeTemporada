    <script>
        function alertme() {
            Swal.fire({
                type: 'error',
                title: 'Oops...',
                text: 'Esperando la aprobacion del administrador',
                footer: ''
            })
        }
        function usuarioNoExiste() {
            Swal.fire({
                type: 'error',
                title: 'Oops...',
                text: 'Usuario o contraseña invalida',
                footer: ''
            })
        }
        function error() {
            Swal.fire({
                type: 'error',
                title: 'Oops...',
                text: 'No se pudo realizar la tarea',
                footer: ''
            })
        }
    </script>

    <script>
